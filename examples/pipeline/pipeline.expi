system in (26, 26) at (2, 2) {
    input data_in : Bitwidth
    output after_n : Bitwidth

    chain compute_blocks at (0, 0) {
        component = compute in (5, 10),
        amount = 7,
        layout = horizontal,
        chain_in = inp,
        chain_out = out
    }

    data_in->compute_blocks[0].inp
    after_n<-compute_blocks[6].out
}